{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "moduleResolution": "Node",
    "jsx": "react",
    "jsxFactory": "Nano.h",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist/ts",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
